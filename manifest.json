{
	"name": "voidTube",
	"description": "Extension",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"scripting",
		"storage"
	],
	"host_permissions": [
		"*://www.youtube.com/*"
	],
	"content_scripts": [
		{
			"matches": ["*://www.youtube.com/*"],
			"js": ["content-script.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	}
}